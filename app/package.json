{
  "name": "mabo",
  "productName": "Mabo",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "",
    "build": "webpack --optimize-minimize --devtool source-map",
    "package": "rm -rf ./dist && npm run osx && npm run win",
    "osx": "electron-packager . Mabo --out=dist/osx --platform=darwin --arch=x64 --electron-version=1.7.5 --prune=true --overwrite",
    "win": "electron-packager . Mabo --out=dist/win --platform=win32 --electron-version=1.7.5 --prune=true --overwrite"
  },
  "main": "./main.js",
  "devDependencies": {
    "aws-sdk": "^2.94.0",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "electron": "^1.7.5",
    "electron-pug": "^1.5.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "gulp-watch": "^4.3.11",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "webpack": "^3.4.1",
    "zangodb": "^1.0.5"
  },
  "dependencies": {
    "aws-sdk": "^2.94.0",
