extends ../layout

block content
    div.container-fluid
        div.card.col-6.offset-3
            div.card-block
                h1.card-title  Project Mabo
                h6.card-subtitle.text-muted trpg:coc online session utility
                form
                    div.form-group
                        label(for="mabo-pw") PassWord
                        div.input-group
                            div.input-group-addon
                                i.fa.fa-key
                            input#mabo-pw.form-control.form-control-sm(type="text",placeholder="")
                    div.btn-group
                        button.btn.btn-sm.btn-primary.btn-outline-primary(type="button",onclick="location.href='#'") login